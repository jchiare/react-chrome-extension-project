{
  "manifest_version": 2,

  "name": "Tab Manager",
  "description": "Messing around with Chrome Extension .. for now",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'", 
  "permissions": [
    "storage",
    "tabs"
  ],
  "icons": { "16": "images/tab16.png",
    "48": "images/tab48.png",
   "128": "images/tab.png" }
}
